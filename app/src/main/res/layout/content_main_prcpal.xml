<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainPrcpal"
    tools:showIn="@layout/app_bar_main_prcpal">

    <ImageView
        android:id="@+id/img_Invntro"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="76dp"
        android:contentDescription="@string/img_invntro_desc"
        android:onClick="open_invntro"
        app:layout_constraintBottom_toTopOf="@+id/txt_prvdres"
        app:layout_constraintEnd_toStartOf="@+id/img_cmpra"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_invntro"
        app:srcCompat="@drawable/logoinventory" />

    <TextView
        android:id="@+id/txt_invntro"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="3dp"
        android:gravity="center"
        android:text="@string/txt_invntro"
        app:layout_constraintBottom_toTopOf="@+id/img_Invntro"
        app:layout_constraintEnd_toStartOf="@+id/txt_cmpras"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_cmpra"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="76dp"
        android:contentDescription="@string/img_cmpras_desc"
        android:onClick="open_prdcto"
        app:layout_constraintBottom_toTopOf="@+id/txt_clntes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img_Invntro"
        app:layout_constraintTop_toBottomOf="@+id/txt_cmpras"
        app:srcCompat="@drawable/prcpalcompras" />

    <TextView
        android:id="@+id/txt_cmpras"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="12dp"
        android:gravity="center"
        android:text="@string/txt_cmpras"
        app:layout_constraintBottom_toTopOf="@+id/img_cmpra"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_invntro"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_clnte"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="215dp"
        android:contentDescription="@string/img_clntes_desc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/img_prvdres"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_prvdres"
        app:srcCompat="@drawable/prcpalclientes" />

    <TextView
        android:id="@+id/txt_clntes"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="12dp"
        android:gravity="center"
        android:text="@string/txt_clntes"
        app:layout_constraintBottom_toTopOf="@+id/img_prvdres"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_prvdres"
        app:layout_constraintTop_toBottomOf="@+id/img_cmpra" />

    <ImageView
        android:id="@+id/img_prvdres"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="215dp"
        android:contentDescription="@string/img_prvdres_desc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img_clnte"
        app:layout_constraintTop_toBottomOf="@+id/txt_clntes"
        app:srcCompat="@drawable/prcpalproveedores" />

    <TextView
        android:id="@+id/txt_prvdres"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="3dp"
        android:gravity="center"
        android:text="@string/txt_prvdres"
        app:layout_constraintBottom_toTopOf="@+id/img_clnte"
        app:layout_constraintEnd_toStartOf="@+id/txt_clntes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_Invntro" />

</androidx.constraintlayout.widget.ConstraintLayout>